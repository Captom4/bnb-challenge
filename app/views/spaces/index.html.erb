<p id="notice"><%= notice %></p>

<h1>Spaces</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Price per night</th>
      <th>Available from</th>
      <th>Available to</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @spaces.each do |space| %>
      <%= render space %>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Space', new_space_path %>
